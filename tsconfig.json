{
	"compilerOptions": {
		"outDir": "dist",
		"declaration": true,
		"target": "ESNext",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"module": "ESNext",
		"moduleResolution": "NodeNext",
		"resolveJsonModule": true,
		"isolatedModules": true,
	},
	"include": ["lib"]
}
